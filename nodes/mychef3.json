{
	"bashrc":{
		"histsize": 90000,
		"histfilesize": 90000
	},
	"sshd_config":{
		"port": 22,
		"passauth": "PasswordAuthentication no",
		"usepam": "UsePam no"
	},
	"wwwconf":{
		"documentroot": "/vagrant",
		"directory": "/vagrant"
	},
	"rbenv": {
		"user_installs": [
			{
				"user": "vagrant",
				"rubies": ["2.0.0-p353"],
				"global": "2.0.0-p353"
			}
		]
	},
	"run_list":[
		"recipe[defset]",
		"ruby_build",
		"rbenv::user"
	]
}
